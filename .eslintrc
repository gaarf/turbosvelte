{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
	"plugins": ["svelte3", "@typescript-eslint"],
	// "ignorePatterns": [],
	"overrides": [{ "files": ["*.svelte"], "processor": "svelte3/svelte3" }],
	"settings": {
		"svelte3/typescript": true
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": "latest"
	},
	"env": {
		"browser": true,
		"es2017": true,
		"node": true
	}
}
